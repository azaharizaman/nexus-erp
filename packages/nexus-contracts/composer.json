{
    "name": "nexus/contracts",
    "description": "Core contract interfaces for the Nexus ERP system",
    "type": "library",
    "license": "proprietary",
    "authors": [
        {
            "name": "Azahari Zaman",
            "email": "azaharizaman@gmail.com"
        }
    ],
    "require": {
