{
    "name": "nexus/uom",
    "description": "Sophisticated UOM Management for Laravel",
    "type": "library",
    "require": {
        "php": "^8.2",
        "brick/math": "^0.11|^0.12|^0.13|^0.14",
        "symfony/polyfill-mbstring": "^1.27",
        "illuminate/support": "^10.0 || ^11.0 || ^12.0 || 13.0.x-dev",
        "nesbot/carbon": "^2.71 || ^3.0",
        "nexus/contracts": "dev-main"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5 || ^12.0",
        "fakerphp/faker": "^1.21",
        "mockery/mockery": "^1.6",
        "doctrine/dbal": "^3.7",
        "ramsey/uuid": "^4.7",
        "league/fractal": "^1.0",
        "composer/installers": "^2.2",
        "spatie/laravel-package-tools": "dev-main",
        "orchestra/testbench": "*"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Nexus\\UomManagement\\": "src/",
            "Nexus\\UomManagement\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nexus\\UomManagement\\Database\\Factories\\": "database/factories/",
            "Nexus\\UomManagement\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Azahari Zaman",
            "email": "azaharizaman@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "config": {
        "allow-plugins": {
            "composer/installers": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nexus\\UomManagement\\UomManagementServiceProvider"
            ]
        }
    }
}
